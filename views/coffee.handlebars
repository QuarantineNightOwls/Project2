    <div class="jumbotron">
        <h1>Quarantine Coffee</h1>
        <p>Quarantine Coffee Cafe</p>
        <br>
        <h2>Customize your Latte</h2>
        <panel>
            <form class="create-form">
                <div class="form-group">
                    <label for="newLatte">Latte Options</label>
                    {{!-- <input type="text" id="newLatte" name="latte"> --}}
                    <select data-placeholder="" id="newLatte" name="latte">
                        <option value="Nothing Selected" selected>Please Select a Latte Option</option>
                        <option value="Blonde Caffè Latte">Blonde Caffè Latte</option>
                        <option value="Caffè Latte">Caffè Latte</option>
                        <option value="Chai Tea Latte">Chai Tea Latte</option>
                        <option value="Cinnamon Dolce Latte">Cinnamon Dolce Latte</option>
                        <option value="Coconutmilk Cascara Latte">Coconutmilk Cascara Latte</option>
                        <option value="Latte">Latte</option>
                        <option value="London Fog Tea Latte">London Fog Tea Latte</option>
                        <option value="Matcha Green Tea Latte">Matcha Green Tea Latte</option>
                        <option value="Matcha Latte">Matcha Latte</option>
                        <option value="Royal English Breakfast Tea Latte">Royal English Breakfast Tea Latte</option>
                    </select>
{{!-- 
                    <label for="drinkAddOns">drink Options</label>
                    {{!-- <input type="text" id="drinkAddons" name="addOn"> --}}
                    <select data-placeholder="" id="drinkAddons" name="addOn">
                        <option value="Nothing Selected" selected>Please Select drink Addons</option>
                        <option value="vanilla">vanilla</option>
                        <option value="honey">honey</option>
                        <option value="cocoa">cocoa</option>
                        <option value="cinnamon">cinnamon</option>
                        <option value="cream">cream</option>
                        <option value="sugar">sugar</option>
                    </select> --}}

                    <button type="submit" class="btn btn-success" id="addLatte">
                        <span class="fa fa-thumbs-up"></span>
                    </button>
                </div>
            </form>
        </panel>
    </div>
    <div class="row">
        <div class="col-sm-6">
            <div class="panel panel-default">
                <div class="panel-heading" id="leftside"><strong>Choose a Latte</strong>
                    <p>Latte is Ready!</p>
                </div>
                <div class="panel-body">

                    <ul>
                        {{#each lattes}}
                        {{#unless drank}}

                        <span><strong> {{this.id}} - {{this.drink_name}}</strong></span>
                        <button type="button" class="drinkLatte btn btn-warning" id="drinkButton" data-id="{{this.id}}"
                            data-newDrank="{{drank}}"><span class="fa fa-usd"></span></button>
                        <hr>
                        {{/unless}}
                        {{/each}}
                    </ul>

                </div>
            </div>
        </div>
        <div class="col-sm-6">
            <div class="panel panel-default">
                <div class="panel-heading" id="rightside"><strong>Latte is Ready!</strong>
                    <p>Please take your Latte</p>
                </div>
                <div class="panel-body">
                    <ul>
                        {{#each lattes}}
                        {{#if drank}}
                        <span><strong> {{this.id}} - {{this.drink_name}}</strong></span>
                        <button type="button" class="trashdrink btn btn-danger" id="trashbutton" data-id="{{this.id}}"
                            data-newDrank="{{devour}}"><span class="fa fa-trash"></span></button>
                        <hr>
                        {{/if}}
                        {{/each}}
                    </ul>



                </div>
            </div>
        </div>

    </div>
    <!--endrow -->
</div>
</div>

